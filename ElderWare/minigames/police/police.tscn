script/source = "extends Minigame

var correct_combination = [9, 9, 7]

var player_sequence = []

func _on_button_0_pressed() -> void:
	player_sequence.append(0)
	_is_three()


func _on_button_1_pressed() -> void:
	player_sequence.append(1)
	_is_three()


