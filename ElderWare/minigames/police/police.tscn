	_is_three()


func _on_button_9_pressed() -> void:
	player_sequence.append(9)
	_is_three()

func _on_lose():
	self.mingame_failed.emit()

func _on_win():
	self.minigame_complete.emit()

func _is_three() -> void:
	if player_sequence.size() == 3: 
		if player_sequence == correct_combination:
			_on_win()
			print(\"ez\")
		else:
			_on_lose()
			print(\"dupa\")
